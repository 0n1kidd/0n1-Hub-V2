-- Booting Library

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

-- Functions

function DestroyGui()

    OrionLib:Destroy()

end

-- Creating Window

local Window = OrionLib:MakeWindow({Name = "0n1 Hub V2 â€¢ Escape Running Head", HidePremium = false, IntroEnabled = false})

-- Creating Tabs

local tab1 = Window:MakeTab({

	Name = "Main",	Icon = "",

	PremiumOnly = false

})

local tab3 = Window:MakeTab({

	Name = "Player",

	Icon = "",

	PremiumOnly = false

})

local tab4 = Window:MakeTab({

	Name = "More",

	Icon = "",

	PremiumOnly = false

})

