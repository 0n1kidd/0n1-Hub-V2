-- Booting Library

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

-- Functions

function DestroyGui()

    OrionLib:Destroy()

end

-- Creating Window

local Window = OrionLib:MakeWindow({Name = "0n1 Hub V2 â€¢ Legends Of Speed", HidePremium = false, IntroEnabled = false})

-- Creating Tabs

local tab1 = Window:MakeTab({

	Name = "Auto Farms",	Icon = "",

	PremiumOnly = false

})

local tab2 = Window:MakeTab({

	Name = "Crystals",

	Icon = "",

	PremiumOnly = false

})

local tab3 = Window:MakeTab({

	Name = "Teleports",

	Icon = "",

	PremiumOnly = false

})

local tab4 = Window:MakeTab({

	Name = "Player",

	Icon = "",

	PremiumOnly = false

})

local tab5 = Window:MakeTab({

	Name = "More",

	Icon = "",

	PremiumOnly = false

})

-- Config Tab1

local Section1 = tab1:AddSection({

	Name = "Main Farms"

})

tab1:AddToggle({

	Name = "Auto Rebirth",

	Default = false,

	Callback = function(state)

   	if state then

           _G.loop = true

           while _G.loop == true do wait()

                 game:GetService("ReplicatedStorage").rEvents.rebirthEvent:FireServer("rebirthRequest")

           end

        else

           _G.loop = false

        end

	end    

})

tab1:AddToggle({

	Name = "Auto Hoops",

	Default = false,

	Callback = function(state)

		if state then

           _G.loop = true

           while _G.loop == true do wait()

                 local children = workspace.Hoops:GetChildren()

                    for i, child in ipairs(children) do

                    if child.Name == "Hoop" then

                          child.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame

                   end

                end

        end

        else

           _G.loop = false

        end

	end    

})

