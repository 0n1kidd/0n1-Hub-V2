-- Booting Library

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

-- Functions

function DestroyGui()

    OrionLib:Destroy()

end

-- Creating Window

local Window = OrionLib:MakeWindow({Name = "0n1 Hub V2 â€¢ Easy Stud Jumps Obby", HidePremium = false, IntroEnabled = false})

-- Creating Tabs

local tab1 = Window:MakeTab({

	Name = "Main",	Icon = "",

	PremiumOnly = false

})

local tab2 = Window:MakeTab({

	Name = "Pets",

	Icon = "",

	PremiumOnly = false

})

local tab3 = Window:MakeTab({

	Name = "Player",

	Icon = "",

	PremiumOnly = false

})

local tab4 = Window:MakeTab({

	Name = "More",

	Icon = "",

	PremiumOnly = false

})

-- Config Tab1

local Section1 = tab1:AddSection({

	Name = "Spawn World"

})

tab1:AddButton({

	Name = "Unlock All Stages (Once)",

	Callback = function()

      	wait(1)

game.Players.LocalPlayer.Character:MoveTo(Vector3.new(200,-5,211))

wait(0.3)

game.Players.LocalPlayer.Character:MoveTo(Vector3.new(200,1266,-871))

wait(0.3)

game.Players.LocalPlayer.Character:MoveTo(Vector3.new(200,1266,-987))

wait(5)

  	end    

})

tab1:AddToggle({

	Name = " Unlock All Stages (Repeats)",

	Default = false,

	Callback = function(state)

		if state then

           _G.loop = true

           while _G.loop == true do wait()

                 wait(1)

game.Players.LocalPlayer.Character:MoveTo(Vector3.new(200,-5,211))

wait(0.3)

game.Players.LocalPlayer.Character:MoveTo(Vector3.new(200,1266,-871))

wait(0.3)

game.Players.LocalPlayer.Character:MoveTo(Vector3.new(200,1266,-987))

wait(5)

           end

        else

           _G.loop = false

        end

	end    

})

