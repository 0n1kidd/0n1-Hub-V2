-- Config Tab3

local section1 = tab3:AddSection({

	Name = "Settings"

})

tab3:AddTextbox({

	Name = "Walk Speed",

	Default = "",

	TextDisappear = false,

	Callback = function(pws)

		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = pws

	end	  

})

tab3:AddTextbox({

	Name = "Jump Power",

	Default = "",

	TextDisappear = false,

	Callback = function(pjp)

		game.Players.LocalPlayer.Character.Humanoid.JumpPower = pjp

	end	  

})

tab3:AddTextbox({

	Name = "Hip Height",

	Default = "",

	TextDisappear = false,

	Callback = function(phh)

		game.Players.LocalPlayer.Character.Humanoid.HipHeight = phh

	end	  

})

tab3:AddButton({

	Name = "Low Graphics",

	Callback = function()

         _G.Settings = {

            Players = {

                ["Ignore Me"] = true, -- Ignore your Character

                ["Ignore Others"] = true -- Ignore other Characters

            },

            Meshes = {

            Destroy = false,

            LowDetail = true

        },

        Images = {

            Invisible = true,

            LowDetail = false,

            Destroy = false,

        },

        ["No Particles"] = true, -- Disables all ParticleEmitter, Trail, Smoke, Fire and Sparkles

        ["No Camera Effects"] = true, -- Disables all PostEffect's (Camera/Lighting Effects)

        ["No Explosions"] = true, -- Makes Explosion's invisible

        ["No Clothes"] = true, -- Removes Clothing from the game

        ["Low Water Graphics"] = true, -- Removes Water Quality

        ["No Shadows"] = true, -- Remove Shadows

        ["Low Rendering"] = true, -- Lower Rendering

        ["Low Quality Parts"] = true -- Lower quality parts

    }

        loadstring(game:HttpGet("https://raw.githubusercontent.com/CasperFlyModz/discord.gg-rips/main/FPSBooster.lua"))()	

	end    

})

tab3:AddButton({

	Name = "Anti Kick",

	Callback = function()

         local vu = game:GetService("VirtualUser")

            game:GetService("Players").LocalPlayer.Idled:connect(function()

                    vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)

                    wait(1)

                vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)

            end)	

	end    

})

-- Config Tab4

local Section1 = tab4:AddSection({

	Name = "Discord"

})

tab4:AddButton({

	Name = "Copy Discord Invite",

	Callback = function()

	    setclipboard("discord.gg/UnjnNVx6Tp")

        OrionLib:MakeNotification({

        Name = "Copied Invite!",

        Content = "Join the Discord by using this invite!!",

        Image = "",

        Time = 1.5

        })

	end    

})

local Section1 = tab4:AddSection({

	Name = "Credits"

})

tab4:AddLabel("0n1kidd#0676")

tab4:AddLabel("MainChat#5720")

tab4:AddLabel("TestAccount#1287")

local Section1 = tab4:AddSection({

	Name = "GUI"

})

tab4:AddButton({

	Name = "Destroy GUI",

	Callback = function()

	    wait(0.5)

	    DestroyGui()

	end    

})

-- Script End

OrionLib:Init()
